{
  "workflow": {
    "name": "image_detection_auto",
    "description": "Single image object detection using auto",
    "version": "1.0",
    "created": "2025-10-30T09:15:03.542361",
    "strategy": "granular",
    "backend": "auto",
    "node_breakdown": {
      "infrastructure": 2,
      "processing": 1,
      "summary": 1,
      "total": 4
    },
    "optimized_parameters": {
      "conf_threshold": 0.25,
      "iou_threshold": 0.7
    },
    "composition_mode": "rule-based",
    "original_request": "simple image blur using gaussian kernel"
  },
  "nodes": [
    {
      "id": "detect_hardware",
      "function": "workflow_nodes.inference_node.detect_available_hardware",
      "inputs": {},
      "outputs": [
        "hardware_status"
      ]
    },
    {
      "id": "download_model",
      "function": "workflow_nodes.directml_model_loader_node.download_yolo_model",
      "inputs": {
        "model_name": "yolov8s.onnx"
      },
      "outputs": [
        "model_path"
      ]
    },
    {
      "id": "process_image",
      "function": "workflow_nodes.image_reader_node.process_image_auto",
      "inputs": {
        "image_path": "image",
        "model_path": {
          "from": "download_model",
          "key": "model_path"
        },
        "conf_threshold": 0.25,
        "iou_threshold": 0.7
      },
      "outputs": [
        "detections",
        "inference_time"
      ]
    },
    {
      "id": "performance_stats",
      "function": "workflow_nodes.performance_stats_node.log_performance_summary",
      "inputs": {
        "total_frames": 1,
        "inference_time": {
          "from": "process_image",
          "key": "inference_time"
        }
      },
      "outputs": []
    }
  ]
}